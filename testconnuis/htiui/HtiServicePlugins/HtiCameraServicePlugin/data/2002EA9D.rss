RESOURCE REGISTRY_INFO theInfo
    {
    // UID for the DLL
    dll_uid = 0x2002EA9D;
    // Declare array of interface info
    interfaces =
        {
        INTERFACE_INFO
            {
            // UID of interface that is implemented
            interface_uid = 0x1020DEB7;
            implementations =
                {
                IMPLEMENTATION_INFO
                    {
                    implementation_uid = 0x2002EA9E;
                    version_no         = 1;
                    display_name       = "Camera Service";
                    default_data       = "Camera";
                    opaque_data        = "";
                    }
                };
            }
        };
    }


